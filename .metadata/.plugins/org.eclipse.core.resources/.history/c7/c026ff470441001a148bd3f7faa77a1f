
public class NumberPyramid {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		for(int i = 1; i <= 5; i++) {
			
			int sp = 5;
			for(;sp >= i; sp--) {
				System.out.print(" ");
			}

			for(int j = 1; j <= 2*i; j++) {
				//System.out.print(" ");
				System.out.printf("%d",i);
			}
			System.out.println();
		}
	}

}
