package Chapter5;

public class PrintingDiamond {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int i = 1;
		String x = "*";
		String space = " ";
		for(; i <= 10; i++) {
			
			System.out.print('*');
			for(int j = 1; j <= i; j++) {
				System.out.print(' ');	
			}
			for(int z = i; z >= 1; z--) {
				System.out.print('*');
			}
			
			
			System.out.println();
		}
		
	}

}
