        int i = 0;
         int result = 0;
        while (i <= 7) {
        	System.out.println(i);
        	
        	
        		if(result % 6 == 0) {
        			
        			result = result - i;
        			i++;
        		}
        		else {
        			result = result + i;
        			
        		}
        		i++;
        }
        System.out.println(result);
}
}
