package Chapter5;

public class PrintingDiamond {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int i = 1;
		String x = "*";
		String space = " ";
		for(; i <= 10; i++) {
			
			System.out.print('*');
			System.out.print(' ');
			
			for(int j = i; j <= 20; j++) {
				System.out.print('*');
				System.out.print(' ');
				//System.out.println();
			}
			System.out.println();
		}
		
		
	}

}
