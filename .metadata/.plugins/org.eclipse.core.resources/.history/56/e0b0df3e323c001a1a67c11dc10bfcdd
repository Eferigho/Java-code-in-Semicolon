package Chapter5;

public class ClassWork {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		/* write a code
		 * 
		 * 
		 */
		int i = 1;
		int sum = 2;
		int result;
		while(i <= 20){
			while(sum <= 20) {
				result = sum + i;
				
				System.out.printf("%d + %d = %d%n",i, sum,result);
				
				if( i + 7 == 21) {
					System.out.printf("This number plus 7 is equal to 21 %d",i);
					break;
				}
				i++;
				
			}
			sum++;
			
			
		}
	}

}
