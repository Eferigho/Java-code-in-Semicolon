
public class PyramidReversal {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		for(int i = 1; i<= 5; i++) {
			
			for(int j = 1; j <= i; j++) {
				System.out.print(i);
			}
			for(int y = 5; y >= i; y-- ) {
				System.out.print(y);
			}
			System.out.println();
		}
	}

}
