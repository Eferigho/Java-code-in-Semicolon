package chapter7;

import java.util.Arrays;

public class TotalSalesTwoDim {

	public static void main(String[] args) {
		
		SaleSlip sp = new SaleSlip();
		
		int[][]comPlusTotalSales = new int[5][4];
		
		
		for(int currentRow = 0; currentRow < comPlusTotalSales.length; currentRow++) {
			System.out.println(Arrays.toString( comPlusTotalSales[currentRow]));
		}
		

	}

}
