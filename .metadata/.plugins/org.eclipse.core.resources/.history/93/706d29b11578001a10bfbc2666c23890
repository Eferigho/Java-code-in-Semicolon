package chapter7;

import java.util.Arrays;

public class TotalSalesTwoDim {

	public static void main(String[] args) {
		
		SaleSlip sp = new SaleSlip();
		
		int[][]comPlusTotalSales = new int[4][5];
		
		
		for(int currentRow = 0; currentRow < comPlusTotalSales.length; currentRow++) {
			System.out.println(comPlusTotalSales[currentRow][currentRow]);
		}
		

	}

}
